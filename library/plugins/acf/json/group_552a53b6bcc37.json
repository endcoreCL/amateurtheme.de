        {
            "key": "field_57fddea01f09c",
            "label": "Wichtiger Hinweis für Twitter",
            "name": "",
            "type": "message",
            "instructions": "",
            "required": 0,
            "conditional_logic": 0,
            "wrapper": {
                "width": "",
                "class": "",
                "id": ""
            },
            "message": "Wenn du die Anzahl der Signale von Twitter nutzen willst, musst du deine Seite kostenlos auf <a href=\"http://opensharecount.com/\" target=\"_blank\">opensharecount.com</a> registrieren. Dies ist notwendig da Twitter die eigene API hierfür deaktiviert hat. Sobald ein Account für deine Domain auf <a href=\"http://opensharecount.com/\" target=\"_blank\">opensharecount.com</a> registriert wurde, werden die Signale ausgelesen. Dies kann bis zu 24 Stunden dauern.",
            "esc_html": 0,
            "new_lines": "wpautop"
        },
        {
            "key": "field_55364dc5a2d72",
            "label": "Anzahl der Signale",
            "name": "socialbuttons_signals",
            "type": "true_false",
            "instructions": "Wenn du diese Option aktivierst werden die Anzahl der Singale pro Netzwerk ausgelesen. Bitte beachte das die Aktualisierung nicht sofort erfolgt, es kann pro Beitrag bis zu 30 Minuten dauern, das liegt daran das wir die Singale in der Datenbank zwischenspeichern\/cachen.",
            "required": 0,
            "conditional_logic": 0,
            "wrapper": {
                "width": 50,
                "class": "",
                "id": ""
            },
            "message": "aktivieren",
            "default_value": 0
        },
